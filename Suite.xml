<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="FirstSuite" verbose="1" parallel="tests" thread-count="1" allow-return-values="true">
		<parameter name="url" value="https://www.google.com.ar" />

	<!-- Test -->
	<test name="iPhone 5 Test">
		<!-- WEBDRIVER OPTIONS -->
		<parameter name="browser" value="chrome" />
		<parameter name="device" value="iPhone 5" />

		<classes>
			<class name="ar.com.informatorio.calidad.test.MyFourthTest" />
		</classes>
	</test>
	<test name="iPhone 6 Test">
		<!-- WEBDRIVER OPTIONS -->
		<parameter name="browser" value="chrome" />
		<parameter name="device" value="iPhone 6" />

		<classes>
			<class name="ar.com.informatorio.calidad.test.MyFourthTest" />
		</classes>
	</test>

	<test name="Nexus 5 Test">
		<!-- WEBDRIVER OPTIONS -->
		<parameter name="browser" value="chrome" />
		<parameter name="device" value="Nexus 5" />

		<classes>
			<class name="ar.com.informatorio.calidad.test.MyFourthTest" />
		</classes>
	</test>
</suite> <!-- Suite -->